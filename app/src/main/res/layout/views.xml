<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:id="@+id/first"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/second"
        android:background="@color/black"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintEnd_toStartOf="@+id/third"
        app:layout_constraintStart_toEndOf="@+id/first"
        android:id="@+id/second"
        android:background="#009688"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintEnd_toStartOf="@+id/fourth"
        app:layout_constraintStart_toEndOf="@+id/second"
        android:id="@+id/third"
        android:background="#7C40F103"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintEnd_toStartOf="@+id/fifth"
        app:layout_constraintStart_toEndOf="@+id/third"
        android:id="@+id/fourth"
        android:background="@color/black"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintEnd_toStartOf="@+id/sixth"
        app:layout_constraintStart_toEndOf="@+id/fourth"
        android:id="@+id/fifth"
        android:background="#1132EA"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        app:layout_constraintEnd_toStartOf="@+id/seventh"
        app:layout_constraintStart_toEndOf="@+id/fifth"
        android:id="@+id/sixth"
        android:background="#FF3D00"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:id="@+id/seventh"
        app:layout_constraintEnd_toEndOf="@+id/eighth"
        app:layout_constraintStart_toEndOf="@+id/sixth"
        android:background="#89C38B"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:id="@+id/eighth"
        app:layout_constraintEnd_toStartOf="@id/ninth"
        app:layout_constraintStart_toEndOf="@+id/seventh"
        android:background="#FBEE7C"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:id="@+id/ninth"
        app:layout_constraintEnd_toEndOf="@id/tenth"
        app:layout_constraintStart_toEndOf="@+id/eighth"
        android:background="#D74CEF"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:id="@+id/tenth"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/ninth"
        android:background="#00FBE4"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        android:id="@+id/aik"
        app:layout_constraintTop_toBottomOf="@+id/first"
        app:layout_constraintBottom_toTopOf="@+id/doo"
        android:background="@color/black"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/aik"
        app:layout_constraintBottom_toTopOf="@+id/teen"
        android:id="@+id/doo"
        android:background="#009688"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/doo"
        app:layout_constraintBottom_toTopOf="@+id/charr"
        android:id="@+id/teen"
        android:background="#E91E63"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/teen"
        app:layout_constraintBottom_toTopOf="@+id/panch"
        android:id="@+id/charr"
        android:background="#673AB7"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/charr"
        app:layout_constraintBottom_toTopOf="@+id/chayy"
        android:id="@+id/panch"
        android:background="#03A9F4"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/panch"
        app:layout_constraintBottom_toTopOf="@+id/satt"
        android:id="@+id/chayy"
        android:background="#8BC34A"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/chayy"
        app:layout_constraintBottom_toTopOf="@+id/aath"
        android:id="@+id/satt"
        android:background="#EA1515"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/satt"
        app:layout_constraintBottom_toTopOf="@+id/noo"
        android:id="@+id/aath"
        android:background="#493EAE"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/aath"
        app:layout_constraintBottom_toTopOf="@+id/das"
        android:id="@+id/noo"
        android:background="#E68D08"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="40dp"
        android:layout_height="0dp"
        app:layout_constraintTop_toBottomOf="@+id/noo"
        app:layout_constraintBottom_toBottomOf="parent"
        android:id="@+id/das"
        android:background="#8BC34A"
        >
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>